	<!-- /////////// Gotta make a whole copy for victory creature/victory evolution creature CUZ YOU CAN'T NEST CONDITIONS (WHYYYY)//// -->
	<template src="proxy/blank.png">
		<matches>
			<match name="Type" value="Creature"/>
			<match name="Rarity" value="Victory Rare"/>
		</matches>
		
		<overlayblocks>
			<artoverlay>
				<location x="0" y="0" />
				<size height="560" width="400" />
			</artoverlay>

			<conditional>
				<switch property="Civilization">
					<case value="Light"> <link block="lightC" /> <link block="vicLight"/></case>
					<case value="Fire"> <link block="fireC" /> <link block="vicFire"/></case>
					<case value="Water"> <link block="waterC" /> <link block="vicWater"/></case>
					<case value="Nature"> <link block="natureC" /> <link block="vicNature"/></case>
					<case value="Darkness"> <link block="darkC" /> <link block="vicDark"/></case>
					<case value="Zero"> <link block="zeroC" /> <link block="vicZero"/></case>
					
					<case value="Darkness/Fire"> <link block="fireDarkC" /> <link block="vicZero"/> </case>
					<case value="Fire/Darkness"> <link block="fireDarkC" /> <link block="vicZero"/></case>
		
					<case value="Nature/Fire"> <link block="fireNatureC" /> <link block="vicZero"/></case>
					<case value="Fire/Nature"> <link block="fireNatureC" /> <link block="vicZero"/></case>
					
					<case value="Light/Water"> <link block="lightWaterC" /> <link block="vicZero"/></case>
					<case value="Water/Light"> <link block="lightWaterC" /> <link block="vicZero"/></case>
					
					<case value="Light/Nature"> <link block="lightNatureC" /> <link block="vicZero"/></case>
					<case value="Nature/Light"> <link block="lightNatureC" /> <link block="vicZero"/></case>
					
					<case value="Darkness/Water"> <link block="waterDarkC" /> <link block="vicZero"/></case>
					<case value="Water/Darkness"> <link block="waterDarkC" /> <link block="vicZero"/></case>

					<case value="Darkness/Nature"> <link block="darkNatureC" /> <link block="vicZero"/></case>
					<case value="Nature/Darkness"> <link block="darkNatureC" /> <link block="vicZero"/></case>
					
					<case value="Light/Darkness"> <link block="lightDarkC" /> <link block="vicZero"/></case>
					<case value="Darkness/Light"> <link block="lightDarkC" /> <link block="vicZero"/></case>
					
					<case value="Light/Fire"> <link block="lightFireC" /> <link block="vicZero"/></case>
					<case value="Fire/Light"> <link block="lightFireC" /> <link block="vicZero"/></case>
					
					<case value="Fire/Water"> <link block="waterFireC" /> <link block="vicZero"/></case>
					<case value="Water/Fire"> <link block="waterFireC" /> <link block="vicZero"/></case>
					
					<case value="Water/Nature"> <link block="waterNatureC" /> <link block="vicZero"/> </case>
					<case value="Nature/Water"> <link block="waterNatureC" /> <link block="vicZero"/> </case>
					<!-- multiciv cases are here for now but using white V-->
					
					<default> <link block="zeroC" /> </default>
				</switch>
			</conditional>
	
			<conditional>
				<if property="Type" contains="Evolution">
					<link block="evo" />
  				</if>
			</conditional>  
			
			<conditional>
				<if property="Rules" contains="{SHIELD TRIGGER}">
					<link block="sTrig" />
  				</if>
			</conditional>
			
			<conditional>
				<if property="Rules" contains="{BLOCKER}">
					<link block="blocker" />
  				</if>
			</conditional>
			<conditional>
				<if property="Rules" contains="Left God Link">
					<link block="leftGod" />
  				</if>
			</conditional>
			<conditional>
				<if property="Rules" contains="Right God Link">
					<link block="rightGod" />
  				</if>
			</conditional>
			<conditional>
				<if property="Rules" contains="Center God Link">
					<link block="centerGod" />
  				</if>
			</conditional>
			<conditional>
				<if property="Type" contains="Exile">
					<link block="exile" />
  				</if>
			</conditional>
		</overlayblocks>
		
		<textblocks>
			<conditional>
				<if property="Civilization" value="Zero">	
					<link block="zerotitle"><property name="Name" /></link>
					<link block="zerorace"><property name="Race" /></link>
					<link block="zerocost"><property name="Cost" /></link>
				</if>
				<else>
					<link block="title"><property name="Name" /></link>
					<link block="race"><property name="Race" /></link>
					<link block="cost"><property name="Cost" /></link>
				</else>
			</conditional>
			<link block="typeC"><property name="Type" /></link>
			<link block="rules"><property name="Rules" /></link>
			<link block="power"><property name="Power" /></link>
		</textblocks>
	</template>
