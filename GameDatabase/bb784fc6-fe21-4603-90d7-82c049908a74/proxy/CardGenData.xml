				<if property="Rules" contains="{BLOCKER}">
					<link block="blocker" />
  				</if>
			</conditional>
			<conditional>
				<if property="Rules" contains="Left God Link">
					<link block="leftGod" />
  				</if>
			</conditional>
			<conditional>
				<if property="Rules" contains="Right God Link">
					<link block="rightGod" />
  				</if>
			</conditional>
			<conditional>
				<if property="Rules" contains="Center God Link">
					<link block="centerGod" />
  				</if>
			</conditional>
			<conditional>
				<if property="Type" contains="Exile">
					<link block="exile" />
  				</if>
			</conditional>
			
			<conditional>
				<if property="Rarity" contains="Victory">
					<link block="vicZero" />
  				</if>
			</conditional>
			
		</overlayblocks>
		
		<textblocks>
			<conditional>
				<if property="Civilization" value="Zero">	
					<link block="zerotitle"><property name="Name" /></link>
					<link block="zerorace"><property name="Race" /></link>
					<link block="zerocost"><property name="Cost" /></link>
				</if>
				<else>
					<link block="title"><property name="Name" /></link>
					<link block="race"><property name="Race" /></link>
					<link block="cost"><property name="Cost" /></link>
				</else>
			</conditional>
			<link block="typeC"><property name="Type" /></link>
			<link block="rules"><property name="Rules" /></link>
			<link block="power"><property name="Power" /></link>
		</textblocks>
	</template>
