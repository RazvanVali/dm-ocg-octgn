			<match name="Type" value="Spell"/>
		</matches>
		
		<overlayblocks>
			<artoverlay>
				<location x="0" y="0" />
				<size height="560" width="400" />
			</artoverlay>

			<conditional>
				<switch property="Civilization">
					<case value="Light"> <link block="lightS" /> </case>
