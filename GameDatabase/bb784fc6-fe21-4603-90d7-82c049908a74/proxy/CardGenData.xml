					<case value="Fire/Nature"> <link block="fireNatureS" /> </case>	
					
					<case value="Light/Water"> <link block="lightWaterS" /> </case>
					<case value="Water/Light"> <link block="lightWaterS" /> </case>

					<case value="Light/Nature"> <link block="lightNatureS" /> </case>
					<case value="Nature/Light"> <link block="lightNatureS" /> </case>
					
					<case value="Darkness/Water"> <link block="waterDarkS" /> </case>
					<case value="Water/Darkness"> <link block="waterDarkS" /> </case>

					<case value="Darkness/Nature"> <link block="darkNatureS" /> </case>
					<case value="Nature/Darkness"> <link block="darkNatureS" /> </case>
					
