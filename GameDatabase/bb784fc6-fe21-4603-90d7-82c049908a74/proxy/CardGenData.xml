					<case value="Darkness/Light"> <link block="lightDarkS" /> </case>
					
					<case value="Light/Fire"> <link block="lightFireS" /> </case>
					<case value="Fire/Light"> <link block="lightFireS" /> </case>
					
					<case value="Fire/Water"> <link block="waterFireS" /> </case>
					<case value="Water/Fire"> <link block="waterFireS" /> </case>
					
					<case value="Water/Nature"> <link block="waterNatureS" /> </case>
					<case value="Nature/Water"> <link block="waterNatureS" /> </case>
					
					<case value = "Fire/Light/Darkness"> <link block="fldBase"> <link block="fldS"> </case>
					<case value = "Fire/Nature/Darkness"> <link block="fndBase"> <link block="fndS"> </case>
					<case value = "Fire/Nature/Light"> <link block="fnlBase"> <link block="fnlS"> </case>
					<case value = "Fire/Water/Darkness"> <link block="fwdBase"> <link block="fwdS"> </case>
					<case value = "Fire/Water/Light"> <link block="fwlBase"> <link block="fwlS"> </case>
					<case value = "Fire/Water/Nature"> <link block="fwnBase"> <link block="fwnS"> </case>
					<case value = "Nature/Light/Darkness"> <link block="nldBase"> <link block="nldS"> </case>
					<case value = "Water/Light/Darkness"> <link block="wldBase"> <link block="wldS"> </case>
					<case value = "Water/Nature/Darkness"> <link block="wndBase"> <link block="wndS"> </case>
